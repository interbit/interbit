{
  "name": "platform-deploy",
  "version": "0.4.44",
  "description": "Boots a platform node",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "start": "npm run deploy",
    "serve": "npm start",
    "build":
      "interbit build --config interbit.prod.config.js --artifacts ./platform",
    "build:dev":
      "interbit build --config interbit.config.js --artifacts ./platform",
    "deploy":
      "cross-env interbit deploy --key-pair src/keys.js --port $PORT --artifacts ./platform",
    "test": "echo \"No tests specified\""
  },
  "keywords": ["Interbit", "blockchain", "node"],
  "dependencies": {
    "cross-env": "^5.1.4",
    "interbit-cli": "0.4.44",
    "lodash": "^4.17.5"
  },
  "author": "BTL GROUP LTD",
  "license": "MIT"
}
